{
  "@context": [
    "https://incf.github.io/neuroshapes/contexts/schema.json",
    {
      "this": "https://neuroshapes.org/commons/transform/shapes/"
    }
  ],
  "@type": "nxv:Schema",
  "imports": [
    "https://neuroshapes.org/commons/entity",
    "https://neuroshapes.org/dash/distribution"
  ],
  "shapes": [
    {
      "@id": "this:TransformShape",
      "@type": "sh:NodeShape",
      "description": "Transform can be either linear or non-linear(deformable).",
      "nodeKind": "sh:BlankNodeOrIRI",
      "and": [
        {
          "node": "https://neuroshapes.org/commons/entity/shapes/EntityShape"
        },
        {
          "property": [
            {
              "path": "schema:distribution",
              "name": "Distribution",
              "description": "Where the data attached to the entity is published.",
              "node": "https://neuroshapes.org/dash/distribution/shapes/DistributionShape",
              "minCount": 1
            }
          ]
        }
      ]
    }
  ],
  "@id": "https://neuroshapes.org/commons/transform"
}