{
  "@context" : [
    "https://incf.github.io/neuroshapes/contexts/schema.json",
    {
      "this" : "https://neuroshapes.org/commons/emptycollection/shapes/"
    }
  ],
  "@type" : "nxv:Schema",
  "imports" : [
    "https://provshapes.org/schemas/emptycollection",
    "https://neuroshapes.org/commons/entity"
  ],
  "shapes" : [
    {
      "@id" : "this:EmptyCollectionShape",
      "@type" : "sh:NodeShape",
      "label" : "An empty collection shape.",
      "comment" : "A size should be provided.",
      "nodeKind" : "sh:BlankNodeOrIRI",
      "and" : [
        {
          "node" : "https://provshapes.org/schemas/emptycollection/shapes/EmptyCollectionShape"
        },
        {
          "node" : "https://neuroshapes.org/commons/entity/shapes/EntityShape"
        },
        {
          "property" : [
            {
              "path" : "schema:size",
              "name" : "Size",
              "description" : "Collection size. The collection can be empty but the size must be greater than 0.",
              "datatype" : "xsd:integer",
              "minExclusive" : 0,
              "minCount" : 1,
              "maxCount" : 1
            }
          ]
        }
      ]
    }
  ]
}