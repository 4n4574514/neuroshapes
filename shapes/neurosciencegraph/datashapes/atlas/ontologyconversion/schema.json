{
  "@context": [
    "https://incf.github.io/neuroshapes/contexts/schema.json",
    {
      "this": "https://neuroshapes.org/commons/ontologyconversion/shapes/"
    }
  ],
  "imports": [
    "https://neuroshapes.org/commons/activity"
  ],
  "@type": "nxv:Schema",
  "shapes": [
    {
      "@id": "this:OntologyConversionShape",
      "@type": "sh:NodeShape",
      "description": "Ontology conversion activity description.",
      "nodeKind": "sh:BlankNodeOrIRI",
      "targetClass": "nsg:OntologyConversion",
      "and": [
        {
          "@id": "https://neuroshapes.org/schemas/neurosciencegraph/commons/activity/v0.1.0/shapes/ActivityShape"
        },
        {
          "property": [
            {
              "path": "prov:used",
              "class": "nsg:ParcellationLabel",
              "minCount": 1,
              "maxCount": 1
            },
            {
              "path": "prov:generated",
              "class": "nsg:ParcellationOntology",
              "minCount": 1,
              "maxCount": 1
            }
          ]
        }
      ]
    }
  ],
  "@id": "https://neuroshapes.org/commons/ontologyconversion"
}