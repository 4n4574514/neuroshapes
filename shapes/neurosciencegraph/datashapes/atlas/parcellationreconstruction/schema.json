{
  "@context": [
    "https://incf.github.io/neuroshapes/contexts/schema.json",
    {
      "this": "https://neuroshapes.org/commons/parcellationreconstruction/shapes/"
    }
  ],
  "imports": [
    "https://neuroshapes.org/commons/activity"
  ],
  "@type": "nxv:Schema",
  "shapes": [
    {
      "@id": "this:ParcellationReconstructionShape",
      "@type": "sh:NodeShape",
      "label": "Parcellation volume reconstruction activity description.",
      "nodeKind": "sh:BlankNodeOrIRI",
      "targetClass": "nsg:ParcellationReconstruction",
      "and": [
        {
          "@id": "https://neuroshapes.org/schemas/neurosciencegraph/commons/activity/v0.1.0/shapes/ActivityShape"
        },
        {
          "property": [
            {
              "path": "prov:used",
              "description": "Parcellation data",
              "class": "nsg:ParcellationImageData",
              "minCount": 1,
              "maxCount": 1
            },
            {
              "path": "prov:generated",
              "class": "nsg:ParcellationVolume",
              "minCount": 1,
              "maxCount": 1
            }
          ]
        }
      ]
    }
  ],
  "@id": "https://neuroshapes.org/commons/parcellationreconstruction"
}