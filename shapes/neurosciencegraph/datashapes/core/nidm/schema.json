  "shapes": [
    {
      "@id": "http://gggg.org",
      "@type": "sh:NodeShape",
      "targetClass": "sio:file",
      "nodeKind": "sh:BlankNodeOrIRI",
      "property": [
        {
          "@id": "http://gggg.org/EchoTime",
          "path": "dicom:EchoTime",
          "name": "EchoTime",
          "description": "Echo time for the scan; the time from the middle of the first pulse to the middle of the echo. Measured in seconds.",
          "datatype": "http://www.w3.org/2001/XMLSchema#double",
          "minInclusive": 0.001,
          "maxInclusive": 0.01
        },
        {
          "@id": "http://gggg.org/EchoTrainLength",
          "path": "dicom:EchoTrainLength",
          "name": "EchoTrainLength",
          "description": "Echo train length for the scan; the number of echoes.",
          "datatype": "http://www.w3.org/2001/XMLSchema#int",
          "minInclusive": 1,
          "maxInclusive": 32
        },
        {
          "@id": "http://gggg.org/FlipAngle",
          "path": "dicom:FlipAngle",
          "name": "FlipAngle",
          "description": "Flip angle for the scan; the amount of rotation the net magnetization undergoes during a pulse. Measured in degrees.",
          "datatype": "http://www.w3.org/2001/XMLSchema#int",
          "minInclusive": 0,
          "maxInclusive": 180
        },
        {
          "@id": "http://gggg.org/InversionTime",
          "path": "dicom:InversionTime",
          "name": "InversionTime",
          "description": "Inversion time for the scan; the time between the 180deg inverting pulse and the 90deg pulse.",
          "datatype": "http://www.w3.org/2001/XMLSchema#double",
          "minInclusive": {
            "@type": "http://www.w3.org/2001/XMLSchema#float",
            "@value": "0.01"
          },
          "maxInclusive": {
            "@type": "http://www.w3.org/2001/XMLSchema#float",
            "@value": "3.0"
          }
        },
        {
          "@id": "http://gggg.org/MagneticFieldStrength",
          "path": "dicom:MagneticFieldStrength",
          "name": "MagneticFieldStrength",
          "description": "Magnetic field strength for the scan. Measured in Tesla.",
          "datatype": "http://www.w3.org/2001/XMLSchema#double",
          "sh:minInclusive": {
            "@type": "http://www.w3.org/2001/XMLSchema#double",
            "@value": "1.5"
          },
          "sh:maxInclusive": {
            "@type": "http://www.w3.org/2001/XMLSchema#double",
            "@value": "3.0"
          }
        },
        {
          "@id": "http://gggg.org/RepetitionTime",
          "path": "dicom:RepetitionTime",
          "name": "RepetitionTime",
          "description": "Repetition time for the scan; the time between successive pulse sequences. Measured in seconds.",
          "datatype": "http://www.w3.org/2001/XMLSchema#double",
          "minInclusive": 0.001,
          "maxInclusive": 10.0
        },
        {
          "path": "nidm:Task",
          "name": "Task",
          "descripion": "The task performed in functional scans.",
          "in": [
            "rest",
            "gonogo",
            "nback"
          ]
        },
        {
          "path": "nidm:hasImageContrastType",
          "name": "ImageContrastType",
          "description": "The image contrast types for the scans.",
          "in": [
            "nidm:T1Weighted",
            "nidm:T2Weighted",
            "nidm:DiffusionWeighted",
            "nidm:FlowWeighted"
          ]
        },
        {
          "path": "nidm:hasImageUsageType",
          "name": "ImageUsageType",
          "description": "The image usage types for the scans.",
          "in": [
            "nidm:Anatomical",
            "nidm:DiffusionWeighted",
            "nidm:Functional"
          ]
        }
      ]
    }
