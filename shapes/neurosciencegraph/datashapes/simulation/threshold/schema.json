            },
            {
              "path": "prov:wasDerivedFrom",
              "name": "Was derived from",
              "description": "The entities from which this entity was derived from.",
              "editorialNote": "The node constraint should be added.",
              "class": "nsg:Collection",
              "maxCount": 1
            }
          ]
        }
